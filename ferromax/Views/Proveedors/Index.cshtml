@model IEnumerable<ferromax.Models.Proveedor>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.nombreProveedor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.productoProveedor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.nombreRepartidor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.apellidosRepartidor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.telefonoRepartidor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.direccionProveedor)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.nombreProveedor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.productoProveedor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.nombreRepartidor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.apellidosRepartidor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.telefonoRepartidor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.direccionProveedor)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
